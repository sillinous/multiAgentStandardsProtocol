    {
      "id": "agent_9_2_1_1_step5",
      "apqc_id": "9.2.1.1",
      "step_number": 5,
      "step_name": "Price Variance Check (5% tolerance)",
      "description": "Check if invoice price is within acceptable variance from PO price",
      "purpose": "Allow minor price differences while flagging significant variances for review",
      "business_value": "Balances processing efficiency with cost control - reduces exception handling by 60%",

      "capabilities": [
        "calculate_price_variance",
        "apply_tolerance_rules",
        "escalate_variance"
      ],

      "input_schema": [
        {
          "name": "invoice_id",
          "type": "string",
          "required": true
        },
        {
          "name": "invoice_amount",
          "type": "currency",
          "required": true
        },
        {
          "name": "po_amount",
          "type": "currency",
          "required": true
        }
      ],

      "output_schema": [
        {
          "name": "variance_percent",
          "type": "number",
          "description": "Price variance as percentage",
          "required": true,
          "example": 3.5
        },
        {
          "name": "variance_amount",
          "type": "currency",
          "description": "Absolute variance amount",
          "required": true,
          "example": 525.00
        },
        {
          "name": "within_tolerance",
          "type": "boolean",
          "description": "Whether variance is within configured tolerance",
          "required": true,
          "example": true
        },
        {
          "name": "approval_required",
          "type": "boolean",
          "description": "Whether additional approval is needed",
          "required": true
        }
      ],

      "decision_rules": [
        {
          "id": "rule_variance_tolerance",
          "name": "Price Variance Tolerance",
          "description": "Maximum allowed price variance percentage",
          "condition": "variance_percent <= tolerance_threshold",
          "action_if_true": "auto_approve",
          "action_if_false": "route_for_approval",
          "configurable": true,
          "default_threshold": 5.0
        },
        {
          "id": "rule_variance_amount_cap",
          "name": "Variance Amount Cap",
          "description": "Maximum variance amount regardless of percentage",
          "condition": "variance_amount <= amount_cap",
          "action_if_true": "proceed",
          "action_if_false": "require_management_approval",
          "configurable": true,
          "default_threshold": 1000.0
        }
      ],

      "next_step_on_success": "agent_9_2_1_1_step6",
      "expected_duration_seconds": 10,
      "user_configurable_fields": [
        "decision_rules.rule_variance_tolerance.default_threshold",
        "decision_rules.rule_variance_amount_cap.default_threshold"
      ],
      "version": "1.0.0",
      "status": "active"
    },


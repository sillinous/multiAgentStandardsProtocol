{
  "apqc_id": "9.2.1.1",
  "apqc_name": "Process invoices and track accounts payable",
  "category": "9.0 - Manage Financial Resources",
  "description": "End-to-end invoice processing workflow from receipt to payment scheduling, including validation, matching, approval, and GL posting.",
  "orchestration_pattern": "sequential",
  "total_steps": 10,
  "estimated_duration_seconds": 1800,
  "compliance_frameworks": ["SOX", "GAAP", "IFRS"],
  "data_retention_days": 2555,

  "agent_cards": [
    {
      "id": "agent_9_2_1_1_step1",
      "apqc_id": "9.2.1.1",
      "apqc_name": "Process invoices and track accounts payable",
      "step_number": 1,
      "step_name": "Invoice Receipt and Validation",
      "description": "Receive invoices from multiple channels (email, EDI, portal) and validate basic data completeness and format",
      "purpose": "Capture invoices and ensure they contain minimum required information before processing",
      "business_value": "Prevents processing errors and reduces manual data entry by 80%",

      "capabilities": [
        "receive_invoice_from_email",
        "receive_invoice_from_edi",
        "receive_invoice_from_portal",
        "extract_invoice_data_ocr",
        "validate_invoice_format",
        "validate_required_fields",
        "detect_duplicate_invoice"
      ],

      "input_schema": [
        {
          "name": "invoice_source",
          "type": "string",
          "description": "Source channel of the invoice",
          "required": true,
          "validation": {"enum": ["email", "edi", "portal", "manual_upload"]},
          "example": "email"
        },
        {
          "name": "invoice_file",
          "type": "file",
          "description": "Invoice document (PDF, image, or EDI file)",
          "required": true,
          "validation": {"max_size_mb": 25, "allowed_types": ["pdf", "png", "jpg", "edi", "xml"]}
        },
        {
          "name": "sender_email",
          "type": "string",
          "description": "Email address of invoice sender (if email source)",
          "required": false,
          "example": "ap@vendor.com"
        }
      ],

      "output_schema": [
        {
          "name": "invoice_id",
          "type": "string",
          "description": "Unique identifier assigned to this invoice",
          "required": true,
          "example": "INV-2025-001234"
        },
        {
          "name": "extracted_data",
          "type": "object",
          "description": "Structured data extracted from invoice",
          "required": true,
          "example": {
            "invoice_number": "VND-12345",
            "invoice_date": "2025-11-20",
            "due_date": "2025-12-20",
            "vendor_name": "Acme Supplies Inc",
            "total_amount": 15750.00,
            "currency": "USD",
            "line_items": []
          }
        },
        {
          "name": "validation_status",
          "type": "string",
          "description": "Result of validation checks",
          "required": true,
          "validation": {"enum": ["valid", "needs_review", "rejected"]},
          "example": "valid"
        },
        {
          "name": "validation_errors",
          "type": "array",
          "description": "List of validation errors if any",
          "required": false,
          "example": ["Missing PO number", "Invalid date format"]
        },
        {
          "name": "confidence_score",
          "type": "number",
          "description": "OCR extraction confidence (0-100)",
          "required": true,
          "example": 95.5
        }
      ],

      "required_integrations": ["aws_textract", "aws_s3"],
      "optional_integrations": ["google_document_ai", "anthropic"],

      "decision_rules": [
        {
          "id": "rule_confidence_threshold",
          "name": "OCR Confidence Threshold",
          "description": "Minimum confidence score to auto-process",
          "condition": "confidence_score >= 85",
          "action_if_true": "proceed_to_next_step",
          "action_if_false": "route_to_manual_review",
          "configurable": true,
          "default_threshold": 85
        },
        {
          "id": "rule_duplicate_check",
          "name": "Duplicate Invoice Detection",
          "description": "Check if invoice was already processed",
          "condition": "duplicate_found == false",
          "action_if_true": "proceed_to_next_step",
          "action_if_false": "reject_as_duplicate",
          "configurable": false
        }
      ],

      "ai_config": {
        "model": "claude-sonnet-4-20250514",
        "prompt_template": "Extract invoice data from the following document. Return structured JSON with: invoice_number, invoice_date, due_date, vendor_name, vendor_address, line_items (description, quantity, unit_price, total), subtotal, tax, total_amount, currency, payment_terms, po_number (if present).",
        "max_tokens": 4000,
        "temperature": 0.1
      },

      "error_handlers": [
        {
          "error_type": "ocr_extraction_failed",
          "description": "OCR could not extract text from document",
          "action": "manual_review",
          "max_retries": 2,
          "escalation_target": "ap_clerk_queue",
          "notification_channels": ["slack"]
        },
        {
          "error_type": "invalid_file_format",
          "description": "Uploaded file is not a supported format",
          "action": "reject",
          "max_retries": 0,
          "escalation_target": null,
          "notification_channels": ["email"]
        }
      ],

      "next_step_on_success": "agent_9_2_1_1_step2",
      "next_step_on_failure": null,
      "expected_duration_seconds": 30,
      "timeout_seconds": 120,
      "sla_target_seconds": 60,
      "requires_audit_log": true,
      "user_configurable_fields": ["decision_rules.rule_confidence_threshold.default_threshold"],
      "version": "1.0.0",
      "status": "active"
    },

    {
      "id": "agent_9_2_1_1_step2",
      "apqc_id": "9.2.1.1",
      "step_number": 2,
      "step_name": "Vendor Verification",
      "description": "Verify vendor exists in master data, validate tax ID, and check vendor status",
      "purpose": "Ensure invoices are only processed for approved, active vendors",
      "business_value": "Prevents fraud and ensures compliance with vendor management policies",

      "capabilities": [
        "lookup_vendor_master",
        "validate_tax_id",
        "check_vendor_status",
        "verify_bank_details",
        "flag_new_vendor"
      ],

      "input_schema": [
        {
          "name": "invoice_id",
          "type": "string",
          "description": "Invoice ID from previous step",
          "required": true
        },
        {
          "name": "vendor_name",
          "type": "string",
          "description": "Vendor name from invoice",
          "required": true
        },
        {
          "name": "vendor_tax_id",
          "type": "string",
          "description": "Vendor tax ID (EIN/VAT)",
          "required": false
        }
      ],

      "output_schema": [
        {
          "name": "vendor_id",
          "type": "string",
          "description": "Internal vendor ID from master data",
          "required": true,
          "example": "VND-00123"
        },
        {
          "name": "vendor_status",
          "type": "string",
          "description": "Current vendor status",
          "required": true,
          "validation": {"enum": ["active", "inactive", "blocked", "pending_approval", "not_found"]},
          "example": "active"
        },
        {
          "name": "verification_result",
          "type": "string",
          "description": "Overall verification outcome",
          "required": true,
          "validation": {"enum": ["verified", "needs_review", "rejected"]},
          "example": "verified"
        },
        {
          "name": "payment_terms",
          "type": "string",
          "description": "Standard payment terms for this vendor",
          "required": false,
          "example": "NET30"
        }
      ],

      "required_integrations": ["netsuite", "quickbooks_online"],
      "optional_integrations": ["sap_s4hana"],

      "decision_rules": [
        {
          "id": "rule_vendor_active",
          "name": "Vendor Active Status",
          "description": "Vendor must be active to process invoice",
          "condition": "vendor_status == 'active'",
          "action_if_true": "proceed_to_next_step",
          "action_if_false": "route_to_vendor_management",
          "configurable": false
        },
        {
          "id": "rule_new_vendor",
          "name": "New Vendor Handling",
          "description": "Route new vendors for setup before processing",
          "condition": "vendor_status != 'not_found'",
          "action_if_true": "proceed_to_next_step",
          "action_if_false": "initiate_vendor_onboarding",
          "configurable": true
        }
      ],

      "error_handlers": [
        {
          "error_type": "erp_connection_failed",
          "description": "Cannot connect to ERP system",
          "action": "retry",
          "max_retries": 3,
          "retry_delay_seconds": 60
        }
      ],

      "next_step_on_success": "agent_9_2_1_1_step3",
      "expected_duration_seconds": 15,
      "timeout_seconds": 60,
      "requires_audit_log": true,
      "version": "1.0.0",
      "status": "active"
    },

    {
      "id": "agent_9_2_1_1_step3",
      "apqc_id": "9.2.1.1",
      "step_number": 3,
      "step_name": "Purchase Order Matching (3-way)",
      "description": "Match invoice to Purchase Order and Goods Receipt for 3-way verification",
      "purpose": "Ensure invoices are only paid for goods/services that were ordered and received",
      "business_value": "Prevents overpayment and ensures spend is authorized - critical SOX control",

      "capabilities": [
        "lookup_purchase_order",
        "lookup_goods_receipt",
        "match_line_items",
        "calculate_matching_score",
        "identify_discrepancies"
      ],

      "input_schema": [
        {
          "name": "invoice_id",
          "type": "string",
          "description": "Invoice ID",
          "required": true
        },
        {
          "name": "po_number",
          "type": "string",
          "description": "Purchase Order number from invoice",
          "required": true
        },
        {
          "name": "line_items",
          "type": "array",
          "description": "Invoice line items to match",
          "required": true
        }
      ],

      "output_schema": [
        {
          "name": "match_status",
          "type": "string",
          "description": "3-way match result",
          "required": true,
          "validation": {"enum": ["full_match", "partial_match", "no_match", "quantity_variance", "price_variance"]},
          "example": "full_match"
        },
        {
          "name": "po_details",
          "type": "object",
          "description": "Purchase order information",
          "required": true
        },
        {
          "name": "gr_details",
          "type": "object",
          "description": "Goods receipt information",
          "required": true
        },
        {
          "name": "variances",
          "type": "array",
          "description": "List of matching variances found",
          "required": false
        },
        {
          "name": "match_score",
          "type": "number",
          "description": "Overall matching score (0-100)",
          "required": true
        }
      ],

      "required_integrations": ["netsuite", "sap_s4hana"],

      "decision_rules": [
        {
          "id": "rule_full_match",
          "name": "Full 3-Way Match",
          "description": "Invoice fully matches PO and GR",
          "condition": "match_status == 'full_match'",
          "action_if_true": "proceed_to_next_step",
          "action_if_false": "evaluate_variance",
          "configurable": false
        }
      ],

      "next_step_on_success": "agent_9_2_1_1_step4",
      "expected_duration_seconds": 45,
      "timeout_seconds": 120,
      "requires_audit_log": true,
      "compliance_frameworks": ["SOX"],
      "version": "1.0.0",
      "status": "active"
    },

    {
